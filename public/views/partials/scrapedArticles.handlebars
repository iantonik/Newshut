{{#each scrapedArticles}}

<li class="card carousel-card">
  <div class="inside-top">
    <img class="carousel-image card-header" src={{image}}>
  </div>
  <div class="art-title">
    <a class="art-link" href="{{link}}">{{title}}</a>
  </div>
  <i class="art-save-btn fas fa-cloud-download-alt fa-2x"></i>
</li>

{{/each}}
